{
  "name": "AwesomeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator 'IPhones 15'",
    "ios:dev": "concurrently \"npm run ios\" \"npm run start-server\"",
    "android:dev": "concurrently \"npm run android\" \"npm run start-server\"",
    "lint": "eslint .",
    "pods": "yarn install && cd ios && pod install && cd ..",
    "start": "react-native start",
    "start-server": "json-server --watch db.json --port 3001",
    "test": "jest"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-toast-message": "^2.1.7",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
